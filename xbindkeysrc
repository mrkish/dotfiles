# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Play/Pause
"playerctl play-pause"
  XF86AudioPlay

# Media Previous
"playerctl previous"
  XF86AudioPrev

# Media Next
"playerctl next"
  XF86AudioNext
